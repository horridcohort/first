<% provide(:title, @user.name) %> <% provide(:title, " Lost Dares") %>
  <div>
  <p><%= link_to @user.name.possessive, dared_user_path(@user) %> <%= link_to "Achieved", dared_user_path(@user) %>
  <%= link_to "Dares", dared_user_path(@user) %> | 
    <strong><%= link_to @user.name.possessive, lost_user_path(@user) %> <%= link_to "Lost", lost_user_path(@user) %>
  <%= link_to "Dares", lost_user_path(@user) %></strong> |
     <%= link_to @user.name.possessive, view_user_path(@user) %> <%= link_to "Issued", view_user_path(@user) %>
  <%= link_to "Dares", view_user_path(@user) %></p>
    <% if @dares.any? %>
     
      <ol class="microposts">
        <%= render @dares, object: @user %>
      </ol>
      <%= will_paginate @dares %>
    <% end %>
  </div>